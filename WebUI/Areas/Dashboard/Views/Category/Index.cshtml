@model List<Category>


<a href="/dashboard/category/create">Create</a>

<br>
@if(Model.Any()){
    foreach(var category in Model){
        <div> 
            <a href="/dashboard/category/update/@category.Id"><span>@category.Name</span></a>
            <a href="/dashboard/category/delete/@category.Id"><span>Delete</span></a>
      </div>
    }
}else{
    <p class="alert alert-warning">Category not found</p>
}